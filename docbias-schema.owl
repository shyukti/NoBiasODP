@prefix : <http://nobias.eu/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dqv: <http://www.w3.org/ns/dqv#> .
@prefix mls: <http://www.w3.org/ns/mls#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix doam: <https://www.openriskmanual.org/ns/doam#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix nobias: <https://nobias.eu/> .
@base <http://nobias.eu/> .

<http://nobias.eu/> rdf:type owl:Ontology ;
                             owl:versionIRI <urn:absolute:version0.1> ;
                             rdfs:comment "DocBias, an ontology to document fake news classification pipelines."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/citation
<http://purl.org/dc/elements/1.1/citation> rdf:type owl:AnnotationProperty ;
                                           <http://purl.org/dc/elements/1.1/identifier> "citation"@en .


###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/identifier
<http://purl.org/dc/elements/1.1/identifier> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#comment
<http://www.w3.org/2004/02/skos/core#comment> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
<http://www.w3.org/2004/02/skos/core#definition> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/Creator
dct:Creator rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain dcat:Dataset ;
            rdfs:range foaf:Person ;
            rdfs:comment "defines creator of the Dataset" .


###  http://purl.org/dc/terms/language
dct:language rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range dct:LinguisticSystem ;
             rdfs:comment "defines the language used in Dataset" .


###  http://purl.org/dc/terms/publisher
dct:publisher rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain dcat:Dataset ;
              rdfs:range foaf:Agent ;
              rdfs:comment "defines publisher of the Dataset" .


###  http://purl.org/dc/terms/temporal
dct:temporal rdf:type owl:ObjectProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range dct:PeriodOfTime ;
             rdfs:comment "defines the period of time from which news is collected" .


###  http://www.w3.org/ns/dcat#theme
dcat:theme rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf owl:topObjectProperty ;
           rdfs:domain dcat:Dataset ;
           rdfs:range <http://www.w3.org/2004/02/skos/core#Concept> ;
           rdfs:comment "defines genre of the Dataset" .


###  http://www.w3.org/ns/dqv#computedOn
dqv:computedOn rdf:type owl:ObjectProperty ;
               rdfs:domain dcat:Dataset ;
               rdfs:range dqv:QualityMeasurement ;
               rdfs:comment "defines measure of metric which is computed on dataset" .


###  http://www.w3.org/ns/dqv#hasQualityAnnotation
dqv:hasQualityAnnotation rdf:type owl:ObjectProperty ;
                         rdfs:domain dcat:Dataset ;
                         rdfs:range dqv:QualityAnnotation ;
                         rdfs:comment "defines annotations related to quality that Dataset has" .


###  http://www.w3.org/ns/dqv#inCategory
dqv:inCategory rdf:type owl:ObjectProperty ;
               rdfs:domain dqv:Dimension ;
               rdfs:range dqv:Category ;
               rdfs:comment "defines Category that given Dimension belongs to" .


###  http://www.w3.org/ns/dqv#inDimension
dqv:inDimension rdf:type owl:ObjectProperty ;
                rdfs:domain dqv:Metric ;
                rdfs:range dqv:Dimension ;
                rdfs:comment "defines Dimension who has given Metric" .


###  http://www.w3.org/ns/dqv#isMeasurementOf
dqv:isMeasurementOf rdf:type owl:ObjectProperty ;
                    rdfs:domain dqv:QualityMeasurement ;
                    rdfs:range dqv:Metric ;
                    rdfs:comment "defines metric which is measured" .


###  http://www.w3.org/ns/mls#executes
mls:executes rdf:type owl:ObjectProperty ;
             rdfs:domain mls:Run ;
             rdfs:range nobias:Classifier ;
             rdfs:comment "defines the algorithm that is executed by the Run" .


###  http://www.w3.org/ns/mls#hasInput
mls:hasInput rdf:type owl:ObjectProperty ;
             rdfs:domain mls:Run ;
             rdfs:range dcat:Dataset ;
             rdfs:comment "defines the Dataset which is used an input for the Run" .


###  http://www.w3.org/ns/mls#realizes
mls:realizes rdf:type owl:ObjectProperty ;
             rdfs:domain mls:Run ;
             rdfs:range mls:Model ;
             rdfs:comment "defines the model that is realized by the Run" .


###  http://www.w3.org/ns/odrl/2/target
odrl:target rdf:type owl:ObjectProperty ;
            rdfs:domain dqv:QualityPolicy ;
            rdfs:range dcat:Dataset ;
            rdfs:comment "defines dataset for which a quality policy is applicable for" .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf owl:topObjectProperty ;
          rdfs:domain prov:Activity ;
          rdfs:range dcat:Dataset ;
          rdfs:comment "defines the Dataset  that is used for provenance" .


###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain nobias:Actor ;
                    rdfs:range prov:Activity ;
                    rdfs:comment "defines the provenance Activity that generates the Actor" .


###  https://nobias.eu/checks
nobias:checks rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain nobias:FactChecker ;
                      rdfs:range nobias:NewsSource ;
                      rdfs:comment "defines Fact Checker checking a News" .


###  https://nobias.eu/classifies
nobias:classifies rdf:type owl:ObjectProperty ;
                          rdfs:domain doam:Output ;
                          rdfs:range nobias:NewsClassification ;
                          rdfs:comment "defines class into which news is classified by the model" .


###  https://nobias.eu/follows
nobias:follows rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       rdfs:domain nobias:SocialMediaUser ;
                       rdfs:range nobias:SocialMediaUser ;
                       rdfs:comment "defines SocialMediaUser following another  SocialMediaUser" .


###  https://nobias.eu/hasActorType
nobias:hasActorType rdf:type owl:ObjectProperty ;
                            rdfs:domain nobias:Actor ;
                            rdfs:range nobias:ActorType ;
                            rdfs:comment "defines Actor type: active or passive for Actor" .


###  https://nobias.eu/hasArchitechture
nobias:hasArchitechture rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf owl:topObjectProperty ;
                                rdfs:domain mls:Model ;
                                rdfs:range nobias:Architecture ;
                                rdfs:comment "defines Architecture of the Model" .


###  https://nobias.eu/hasBiasRecipient
nobias:hasBiasRecipient rdf:type owl:ObjectProperty ;
                                owl:inverseOf nobias:hasBiasSource ;
                                rdfs:domain nobias:BiasSourceAgent ;
                                rdfs:range nobias:BiasRecipientAgent ;
                                rdfs:comment "describes the entity that has a bias recipient" .


###  https://nobias.eu/hasBiasSource
nobias:hasBiasSource rdf:type owl:ObjectProperty ;
                             rdfs:domain nobias:BiasRecipientAgent ;
                             rdfs:range nobias:BiasSourceAgent ;
                             rdfs:comment "describes the entity that has a bias source" .


###  https://nobias.eu/hasBiasType
nobias:hasBiasType rdf:type owl:ObjectProperty ;
                           rdfs:domain nobias:BiasEvent ;
                           rdfs:range nobias:BiasType ;
                           rdfs:comment "describes the type of bias detected during the Bias Event" .


###  https://nobias.eu/hasClassifier
nobias:hasClassifier rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf owl:topObjectProperty ;
                             rdfs:domain mls:Model ;
                             rdfs:range nobias:Classifier ;
                             rdfs:comment "defines Classification algorithm used to  classify News" .


###  https://nobias.eu/hasContentType
nobias:hasContentType rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain dcat:Dataset ;
                              rdfs:range nobias:NewsContentType ;
                              rdfs:comment "defines the type of content is used in news articles like text, images, videos etc." .


###  https://nobias.eu/hasDetectionMethod
nobias:hasDetectionMethod rdf:type owl:ObjectProperty ;
                                  rdfs:domain nobias:BiasEvent ;
                                  rdfs:range nobias:BiasDetectionMethod ;
                                  rdfs:comment "describes the detection method for detecting bias during Bias Event" .


###  https://nobias.eu/hasEvalMetrics
nobias:hasEvalMetrics rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf owl:topObjectProperty ;
                              rdfs:domain mls:ModelEvaluation ;
                              rdfs:range mls:EvaluationProcedure ;
                              rdfs:comment "defines the type of Metrics that is used to compare different Runs" .


###  https://nobias.eu/hasHyperparameter
nobias:hasHyperparameter rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                 rdfs:domain mls:Process ;
                                 rdfs:range mls:HyperParameter ;
                                 rdfs:comment "defines the weights need to be trained for classification" .


###  https://nobias.eu/hasInput
nobias:hasInput rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain mls:Model ;
                        rdfs:range dcat:Dataset ;
                        rdfs:comment "defines input to the Model in form of  Dataset" ,
                                     "defines input to the Model in form of Datase" .


###  https://nobias.eu/hasLearningMethod
nobias:hasLearningMethod rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf owl:topObjectProperty ;
                                 rdfs:domain mls:HyperParameter ;
                                 rdfs:range nobias:LearningMethod ;
                                 rdfs:comment "defines the method used to train or calculate the weights or hyperparameters" .


###  https://nobias.eu/hasMediaPlatform
nobias:hasMediaPlatform rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf owl:topObjectProperty ;
                                rdfs:domain dcat:Dataset ;
                                rdfs:range nobias:MediaPlatform ;
                                rdfs:comment "defines Media Platform that is used by users who share news" .


###  https://nobias.eu/hasModelComponents
nobias:hasModelComponents rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdfs:domain doam:Predictive_Model ;
                                  rdfs:range doam:Model_Components ;
                                  rdfs:comment "defines components of Model based on doam ontology" .


###  https://nobias.eu/hasModelType
nobias:hasModelType rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain mls:Model ;
                            rdfs:range doam:Predictive_Model ;
                            rdfs:comment "defines the type of Model based on doam ontology" .


###  https://nobias.eu/hasOptimizer
nobias:hasOptimizer rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf owl:topObjectProperty ;
                            rdfs:domain nobias:Classifier ;
                            rdfs:range nobias:OptimizationTechnique ;
                            rdfs:comment "defines Optimization algorithm used to optimize Classifier results" .


###  https://nobias.eu/hasOutput
nobias:hasOutput rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain mls:Model ,
                                     mls:Run ;
                         rdfs:range mls:ModelEvaluation ,
                                    doam:Output ;
                         rdfs:comment "defines output of the Model as doam:Output class" ,
                                      "defines the output of the Model as a result of the Run" .


###  https://nobias.eu/hasRating
nobias:hasRating rdf:type owl:ObjectProperty ;
                         rdfs:domain dcat:Dataset ;
                         rdfs:range nobias:RatingScale ;
                         rdfs:comment "defines Rating Scale is used to rate publishers" .


###  https://nobias.eu/hasRecipientParticipant
nobias:hasRecipientParticipant rdf:type owl:ObjectProperty ;
                                       rdfs:domain nobias:BiasEvent ;
                                       rdfs:range nobias:BiasRecipient ;
                                       rdfs:comment "describes the recipient participant participating in a Bias Event" .


###  https://nobias.eu/hasSource
nobias:hasSource rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain nobias:News ;
                         rdfs:range nobias:NewsSource ;
                         rdfs:comment "defines News Source  for a News" .


###  https://nobias.eu/hasSourceParticipant
nobias:hasSourceParticipant rdf:type owl:ObjectProperty ;
                                    rdfs:subPropertyOf owl:topObjectProperty ;
                                    rdfs:domain nobias:BiasEvent ;
                                    rdfs:range nobias:BiasSource ;
                                    rdfs:comment "describes the source participant participating in a Bias Event" .


###  https://nobias.eu/hasTrainingProcess
nobias:hasTrainingProcess rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf owl:topObjectProperty ;
                                  rdfs:domain mls:Model ;
                                  rdfs:range mls:Process ;
                                  rdfs:comment "defines the training process of training the model" .


###  https://nobias.eu/isPartOf
nobias:isPartOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain mls:Experiment ;
                        rdfs:range mls:Study ;
                        rdfs:comment "defines the Study which conducts the experiment on given Dataset" .


###  https://nobias.eu/performsRecipientRole
nobias:performsRecipientRole rdf:type owl:ObjectProperty ;
                                     rdfs:domain mls:Model ,
                                                 nobias:Actor ;
                                     rdfs:range nobias:BiasRecipientAgent ;
                                     rdfs:comment "describes the entity that performs recipient role for the recipient agent" .


###  https://nobias.eu/performsSourceRole
nobias:performsSourceRole rdf:type owl:ObjectProperty ;
                                  rdfs:domain mls:Model ,
                                              nobias:Actor ;
                                  rdfs:range nobias:BiasSourceAgent ;
                                  rdfs:comment "describes the entity that performs source role for the source agent" .


###  https://nobias.eu/preparesData
nobias:preparesData rdf:type owl:ObjectProperty ;
                            rdfs:domain nobias:Pre-processing ;
                            rdfs:range dcat:Dataset ;
                            rdfs:comment "defines files that \\\\ are used in preparing the Data for training to be used by Model" .


###  https://nobias.eu/providesRecipientRole
nobias:providesRecipientRole rdf:type owl:ObjectProperty ;
                                     rdfs:domain nobias:BiasRecipient ;
                                     rdfs:range nobias:BiasRecipientAgent ;
                                     rdfs:comment "describes the entity that provides recipient role for the recipient agent" .


###  https://nobias.eu/providesSourceRole
nobias:providesSourceRole rdf:type owl:ObjectProperty ;
                                  rdfs:domain nobias:BiasSource ;
                                  rdfs:range nobias:BiasSourceAgent ;
                                  rdfs:comment "describes the entity that provides source role for the source agent" .


###  https://nobias.eu/publishes
nobias:publishes rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain nobias:NewsSource ;
                         rdfs:range nobias:News ;
                         rdfs:comment "defines News published by a News Source" .


###  https://nobias.eu/sharedBy
nobias:sharedBy rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf owl:topObjectProperty ;
                        rdfs:domain nobias:News ;
                        rdfs:range nobias:SharingBehaviour ;
                        rdfs:comment "defines News that is  being shared" .


###  https://nobias.eu/shares
nobias:shares rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      rdfs:domain nobias:SocialMediaUser ;
                      rdfs:range nobias:SharingBehaviour ;
                      rdfs:comment "defines SocialMediaUser that shares the News" .


###  https://www.openriskmanual.org/ns/doam#has_RuleSet
doam:has_RuleSet rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf owl:topObjectProperty ;
                 rdfs:domain doam:Rule_Set_Model ;
                 rdfs:range doam:Rule_Set ;
                 rdfs:comment "defines the rules that Rule set model has" .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/Issued
dct:Issued rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:domain dcat:Dataset ;
           rdfs:range rdfs:Literal ;
           rdfs:comment "defines date on which Dataset is published" .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:DatatypeProperty ;
                rdfs:domain dcat:Dataset ;
                rdfs:range rdfs:Literal ;
                rdfs:comment "gives description of dataset" .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:DatatypeProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range rdfs:Literal ;
             rdfs:comment "defines date on which Dataset is modified" .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:DatatypeProperty ;
          rdfs:domain dcat:Dataset ;
          rdfs:range rdfs:Literal ;
          rdfs:comment "defines title of the Dataset" .


###  http://www.w3.org/ns/dcat#Keyword
dcat:Keyword rdf:type owl:DatatypeProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range rdfs:Literal ;
             rdfs:comment "defines Keywords describing the Dataset" .


###  http://www.w3.org/ns/dqv#ExpectedDatatype
dqv:ExpectedDatatype rdf:type owl:DatatypeProperty ;
                     rdfs:domain dqv:Metric ;
                     rdfs:range rdfs:Literal ;
                     rdfs:comment "defines expected datatype of metric" .


###  http://www.w3.org/ns/dqv#Value
dqv:Value rdf:type owl:DatatypeProperty ;
          rdfs:domain dqv:QualityMeasurement ;
          rdfs:range rdfs:Literal ;
          rdfs:comment "defines value of the metric measured" .


###  https://nobias.eu/TotalStories
nobias:TotalStories rdf:type owl:DatatypeProperty ;
                            rdfs:domain dcat:Dataset ;
                            rdfs:range xsd:integer ;
                            rdfs:comment "defines total number of news stories in  Dataset" .


###  https://nobias.eu/hasBiasScore
nobias:hasBiasScore rdf:type owl:DatatypeProperty ;
                            rdfs:domain nobias:BiasEvent ;
                            rdfs:range xsd:float ;
                            rdfs:comment "stores the measure of bias calculated during detection method" .


###  https://nobias.eu/hasCredibilityClass
nobias:hasCredibilityClass rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf owl:topDataProperty ;
                                   rdfs:domain nobias:NewsSource ;
                                   rdfs:range xsd:string ;
                                   rdfs:comment "defines Credibility Class of News Source given by" .


###  https://nobias.eu/hasCredibilityScore
nobias:hasCredibilityScore rdf:type owl:DatatypeProperty ;
                                   rdfs:subPropertyOf owl:topDataProperty ;
                                   rdfs:domain nobias:NewsSource ;
                                   rdfs:range xsd:float ;
                                   rdfs:comment "defines Credibility Score of News Source given by Factchecker" .


###  https://nobias.eu/hasFactCheckerUrl
nobias:hasFactCheckerUrl rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf owl:topDataProperty ;
                                 rdfs:domain nobias:FactChecker ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "defines URL of the Fact-checking website or organization" .


###  https://nobias.eu/hasGoal
nobias:hasGoal rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf owl:topDataProperty ;
                       rdfs:domain mls:Model ;
                       rdfs:range xsd:string ;
                       rdfs:comment "defines goal of the Model" .


###  https://nobias.eu/hasGroundTruth
nobias:hasGroundTruth rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf owl:topDataProperty ;
                              rdfs:domain nobias:News ;
                              rdfs:range xsd:int ;
                              rdfs:comment "defines the ground truth of news whether it is fake or real" .


###  https://nobias.eu/hasLabel
nobias:hasLabel rdf:type owl:DatatypeProperty ;
                        rdfs:domain nobias:NewsClassification ;
                        rdfs:range xsd:string ;
                        rdfs:comment "defines News Label which is classified by the model" .


###  https://nobias.eu/hasLimitation
nobias:hasLimitation rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf owl:topDataProperty ;
                             rdfs:domain mls:Model ;
                             rdfs:range xsd:string ;
                             rdfs:comment "defines Limitations of the Model" .


###  https://nobias.eu/hasMeasureValue
nobias:hasMeasureValue rdf:type owl:DatatypeProperty ;
                               rdfs:subPropertyOf owl:topDataProperty ;
                               rdfs:domain mls:EvaluationProcedure ;
                               rdfs:range xsd:float ;
                               rdfs:comment "defines value of specific Evaluation Procedure" .


###  https://nobias.eu/hasNewsContent
nobias:hasNewsContent rdf:type owl:DatatypeProperty ;
                              rdfs:subPropertyOf owl:topDataProperty ;
                              rdfs:domain nobias:News ;
                              rdfs:range xsd:string ;
                              rdfs:comment "define News content  in text form" .


###  https://nobias.eu/hasNewsID
nobias:hasNewsID rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:domain nobias:News ;
                         rdfs:range xsd:string ;
                         rdfs:comment "defines unique News Identifier" .


###  https://nobias.eu/hasNewsScore
nobias:hasNewsScore rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf owl:topDataProperty ;
                            rdfs:domain doam:Output ;
                            rdfs:range xsd:int ;
                            rdfs:comment "defines score given by Model to the News which is classified" .


###  https://nobias.eu/hasNewsTitle
nobias:hasNewsTitle rdf:type owl:DatatypeProperty ;
                            rdfs:subPropertyOf owl:topDataProperty ;
                            rdfs:domain nobias:News ;
                            rdfs:range xsd:string ;
                            rdfs:comment "defines News Title" .


###  https://nobias.eu/hasNewsUrl
nobias:hasNewsUrl rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf owl:topDataProperty ;
                          rdfs:domain nobias:News ;
                          rdfs:range xsd:string ;
                          rdfs:comment "defines News URL" .


###  https://nobias.eu/hasSocialMediaUrl
nobias:hasSocialMediaUrl rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf owl:topDataProperty ;
                                 rdfs:domain nobias:SocialMediaUser ;
                                 rdfs:range xsd:string ;
                                 rdfs:comment "defines URL of Social Media that a User uses" .


###  https://nobias.eu/hasSpontaneity
nobias:hasSpontaneity rdf:type owl:DatatypeProperty ;
                              rdfs:domain dcat:Dataset ;
                              rdfs:range xsd:boolean ;
                              rdfs:comment "defines whether dataset was spontaneously collected from Media Platforms or computer-generated" .


###  https://nobias.eu/isAvailable
nobias:isAvailable rdf:type owl:DatatypeProperty ;
                           rdfs:domain dcat:Dataset ;
                           rdfs:range xsd:boolean ;
                           rdfs:comment "defines whether dataset is publicly available" .


###  https://nobias.eu/sourceUrl
nobias:sourceUrl rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf owl:topDataProperty ;
                         rdfs:domain nobias:NewsSource ;
                         rdfs:range xsd:string ;
                         rdfs:comment "defines URL for News Source" .


###  https://nobias.eu/specificationDescription
nobias:specificationDescription rdf:type owl:DatatypeProperty ;
                                        rdfs:domain mls:EvaluationProcedure ;
                                        rdfs:range xsd:string ;
                                        rdfs:comment "defines the specification of the measured value in Evaluation Procedure" .


###  https://nobias.eu/timesShared
nobias:timesShared rdf:type owl:DatatypeProperty ;
                           rdfs:subPropertyOf owl:topDataProperty ;
                           rdfs:domain nobias:SharingBehaviour ;
                           rdfs:range xsd:int ;
                           rdfs:comment "defines how many times a news is shared by a given user" .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/LinguisticSystem
dct:LinguisticSystem rdf:type owl:Class ;
                     rdfs:comment "Language is the language used in news content."@en ;
                     rdfs:label "Language" .


###  http://purl.org/dc/terms/PeriodOfTime
dct:PeriodOfTime rdf:type owl:Class ;
